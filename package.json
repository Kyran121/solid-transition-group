    "@rollup/plugin-node-resolve": "15.0.1",
    "astro": "^2.0.17",
    "prettier": "^2.8.4",
    "rollup": "^3.18.0",
    "solid-js": "^1.6.11",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@solid-primitives/refs": "^1.0.0",
